<!DOCTYPE html>
<html>
<head>
  <title>Teamy</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <div class="col-md-2">
    <p><%= link_to "Home", '#' %>
    <p><%= link_to "Task View", tasks_path %></p>
    <p><%= link_to "Meeting View", meetings_path %></p>
  </div>
  <div class="col-md-10">
    <p id="notice"><%= notice %></p>
    <p id="alert"><%= alert %></p>
    <%= yield %>
  </div>
</div>

</body>
</html>
